console.log("12"+1) //Lo concatena
console.log("11"-1) //Pasa el 11 a numero

console.log("Marina" && "Hoffmann") //Como no es lazy tiene que chequear el 2do elemento
console.log("Marina" || "Hoffmann") //Es lazy

console.log(10/5) //Cociente
console.log(10%5) //Resto
console.log(2**2) //Potencia

console.log(1==1) //True
console.log(1!=1) //False
console.log(1=="1") //True, comparacion debil
console.log(1==="1") //False, comparacion estricta
console.log("1"!==1) //True

